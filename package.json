{
  "name": "web",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "set NODE_OPTIONS='--max-old-space-size=8192' webpack --config config/webpack.prod.js",
    "build:css": "tailwindcss -i src/assets/css/tailwind.css -o src/assets/css/index.css",
    "clean": "npm run clean:css && npm run clean:js",
    "clean:css": "rimraf ./src/assets/css/index.css",
    "clean:js": "rimraf ./build",
    "dev": "npm run dev:upgrade && npm run prebuild",
    "prettify": "prettier --ignore-path \"./.prettierignore\" --write \"./**/*.{js,jsx,json,html,ts,tsx}\"",
    "dev:upgrade": "ncu -u && npm i --force",
    "first-time": "npm i -g npm-check-updates && npm run dev",
    "lint:fix": "eslint ./src --fix",
    "prebuild": "npm run clean && npm run build:css",
    "start": "npm run build:css && webpack serve --config config/webpack.dev.js"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@headlessui/react": "^1.7.17",
    "@jsonforms/core": "3.1.0",
    "@jsonforms/react": "3.1.0",
    "@mui/icons-material": "^5.14.18",
    "@mui/lab": "^5.0.0-alpha.153",
    "@mui/material": "^5.14.18",
    "@popperjs/core": "^2.11.8",
    "ajv": "^8.12.0",
    "axios": "^1.6.2",
    "dayjs": "^1.11.10",
    "echarts": "^5.4.3",
    "echarts-for-react": "^3.0.2",
    "history": "^5.3.0",
    "html-to-image": "^1.11.11",
    "jwt-decode": "^4.0.0",
    "lodash": "^4.17.21",
    "nanoid": "^5.0.3",
    "react": "^18.2.0",
    "react-ace": "^10.1.0",
    "react-datepicker": "^4.23.0",
    "react-dom": "^18.2.0",
    "react-popper": "^2.3.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.20.0",
    "react-select": "^5.8.0",
    "react-to-print": "^2.14.15",
    "reactflow": "^11.10.1",
    "redux": "^4.2.1",
    "redux-thunk": "^2.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.23.4",
    "@babel/preset-env": "^7.23.5",
    "@babel/preset-react": "^7.23.3",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.11",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@types/jest": "^29.5.10",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.39",
    "@types/react-dom": "^18.2.17",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/parser": "^6.13.1",
    "autoprefixer": "^10.4.16",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "clean-webpack-plugin": "*",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.54.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.7.0",
    "json-minimizer-webpack-plugin": "^4.0.0",
    "lint-staged": "^15.1.0",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.31",
    "postcss-loader": "^7.3.3",
    "prettier": "^3.1.0",
    "prettier-eslint": "^16.1.2",
    "react-refresh": "^0.14.0",
    "style-loader": "^3.3.3",
    "tailwind-scrollbar": "^3.0.5",
    "tailwindcss": "^3.3.5",
    "terser-webpack-plugin": "^5.3.9",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.10.0"
  },
  "eslintConfig": {
    "ignorePatterns": [
      "src/assets/*"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "prettier"
    ],
    "rules": {
      "quotes": [
        "error"
      ],
      "semi": 1
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
